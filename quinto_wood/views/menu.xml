    <menuitem name="Gestión de Proveedores" id="quinto_wood.menu_proveedores" parent="quinto_wood.menu_root"/>
    <menuitem name="Gestión de Envíos" id="quinto_wood.menu_envios" parent="quinto_wood.menu_root"/>
    <menuitem name="Gestión de Ventas" id="quinto_wood.menu_ventas" parent="quinto_wood.menu_root"/>

    <!-- Acciones -->
