              action="quinto_wood_producto_action"/>
    <menuitem name="Lista de Proveedores" id="quinto_wood.menu_proveedores_list" parent="quinto_wood.menu_proveedores"
              action="quinto_wood_proveedor_action"/>
    <menuitem name="Lista de Envíos" id="quinto_wood.menu_envios_list" parent="quinto_wood.menu_envios"
              action="quinto_wood_envios_action"/>
    <menuitem name="Lista de Ventas" id="quinto_wood.menu_ventas_list" parent="quinto_wood.menu_ventas"
              action="quinto_wood_ventas_action"/>
